@startuml
package "Array: ['Maria', 'Jose', 'Ana']" {
    [Index 0: 'Maria'] as a0
    [Index 1: 'Jose'] as a1
    [Index 2: 'Ana'] as a2
}

note left of a0
  unshift('Rosa')
  ➜ Add to START
end note

note right of a2
  push('Pedro')
  ➜ Add to END
end note

note left of a0
  shift()
  ➜ Remove from START
end note

note right of a2
  pop()
  ➜ Remove from END
end note
@enduml
